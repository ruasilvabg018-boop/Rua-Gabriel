     <%-include('../cabecalho.ejs')%>
     <header>
        <h3>Adicionar Novo Cliente</h3>
      </header>
      <div class="content">
        <form id="form-Cliente" autocomplete="off" action="/Cliente/add/ok"
          enctype="multipart/form-data" method="post">
          <div class="grid">
            <div>
              <label for="nome">Nome do Cliente</label>
              <input id="nome" name="nome" type="text" required>
            </div>
            <div>
              <label for="foto">Foto do Cliente</label>
              <input id="foto" name="foto" type="file">
            </div>
            <div>
              <label for="tempodetreino">Tempo de treino do Cliente (em anos)</label>
              <input id="tempodetreino" name="tempodetreino" type="number" required>
            </div>
            <div>
              <label for="email">Email do Cliente</label>
              <input id="email" name="email" type="email" required>
            </div>
          <footer>
            <a type="button" class="btn ghost" id="btn-cancelar" href="/Clientes/lst">Cancelar</a>
            <button type="submit" class="btn primary">Adicionar Clientes</button>
          </footer>
        </form>
      </div>

      <%-include('../rodape.ejs')%>